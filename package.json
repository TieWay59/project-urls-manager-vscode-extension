{
	"name": "project-urls-manager",
	"displayName": "Project URLs Manager",
	"description": "Manage all urls of your project in one place",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "project-urls-manager.openProjectUrlsManager",
				"title": "Open: Project URLs Manager"
			},
			{
				"command": "project-urls-manager.syncProjectUrls",
				"title": "Sync: Project URLs"
			}
		]
	},
	"scripts": {
		"dev": "npm-run-all clean compile copy-assets watch",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm-run-all compile copy-assets lint",
		"test": "node ./out/test/runTest.js",
		"copy-assets": "ts-node copy-assets.ts",
		"clean": "rimraf out/*"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/shelljs": "^0.8.8",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"npm-run-all": "^4.1.5",
		"shelljs": "^0.8.4",
		"ts-node": "^8.10.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
